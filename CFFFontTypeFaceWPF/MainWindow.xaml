<Window x:Class="CFFFontTypeFaceWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:CustPanel="clr-namespace:CFFFont.CustomFontViewer;assembly=CFFFont"
        xmlns:local="clr-namespace:CFFFontTypeFaceWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <Grid>
        <Button x:Name="LoadFile" Content="Load CFF File"
                Width="150" Height="25" HorizontalAlignment="Left"
                VerticalAlignment="Top" Margin="5"
                Click="LoadFile_Click">
            
        </Button>
        <CustPanel:CustomPanel x:Name="CustPanel" Height="400" Width="700">
            
        </CustPanel:CustomPanel>
    </Grid>
</Window>
